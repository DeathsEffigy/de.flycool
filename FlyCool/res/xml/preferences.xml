<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <PreferenceCategory
        android:key="pref_key_altitudes"
        android:title="@string/pref_title_altitudes" >
        <EditTextPreference
            android:defaultValue="50"
            android:inputType="number"
            android:key="pref_key_min_altitude_gnd"
            android:summary="@string/pref_summary_min_altitude_gnd"
            android:title="@string/pref_title_min_altitude_gnd" />
        <EditTextPreference
            android:defaultValue="2950"
            android:inputType="number"
            android:key="pref_key_max_altitude_msl"
            android:summary="@string/pref_summary_max_altitude_msl"
            android:title="@string/pref_title_max_altitude_msl" />
    </PreferenceCategory>

</PreferenceScreen>
